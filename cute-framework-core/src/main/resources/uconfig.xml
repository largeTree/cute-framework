<?xml version="1.0" encoding="UTF-8"?>
<config>

	<uconfig env="dev">
		<domain id="db" type="properties" order="lastOne">
			<item path="classpath:/config/db.properties" />
		</domain>
		<domain id="msg" type="properties" order="replace">
			<item path="classpath*:/msg/msg.properties" />
		</domain>
	</uconfig>

	<uconfig env="produce">
		<domain id="db" type="properties" order="lastOne">
			<item path="classpath:/web/etc/db.properties" />
		</domain>
		<domain id="msg" type="properties" order="replace">
			<item path="classpath:/web/etc/msg.properties" />
		</domain>
	</uconfig>

</config>