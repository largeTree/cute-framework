<?xml version="1.0" encoding="UTF-8"?>
<env>
	<!-- 应用名称 -->
	<app-name>${rootArtifactId}</app-name>
	<!-- 服务器id -->
	<server-id>${rootArtifactId}-dev</server-id>
	<!-- 缓存类型 -->
	<mc-server-type>redis</mc-server-type>
	<!-- 序列生成器类型 -->
	<seq-type>db</seq-type>
	<!-- 序列在redis中的索引 -->
	<seq-db-idx>0</seq-db-idx>
	<!-- 是否调试模式 -->
	<is-debug>true</is-debug>
	<!-- 是否测试模式 -->
	<is-test>true</is-test>
	<!-- web服务器索引 -->
	<web-index>0</web-index>
	<!-- 定时任务是否开启 -->
	<timer-task-open>true</timer-task-open>
	<!-- 版本 -->
	<version>${version}</version>
</env>